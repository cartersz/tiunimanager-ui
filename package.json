{
  "name": "ticp",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "cross-env VITE_MOCK=1 vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint . --cache",
    "prepare": "is-ci || husky install",
    "gen:api": "yarn openapi-generator-cli generate"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-form": "^1.32.4",
    "@ant-design/pro-table": "^2.43.6",
    "antd": "^4.16.8",
    "axios": "^0.21.1",
    "copy-to-clipboard": "^3.3.1",
    "cron-parser": "^3.5.0",
    "i18next": "^20.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "nprogress": "^0.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hooks-global-state": "^1.0.1",
    "react-i18next": "^11.11.3",
    "react-js-cron": "^1.2.1",
    "react-query": "^3.19.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.3.7",
    "@rollup/pluginutils": "^4.1.1",
    "@types/faker": "^5.5.7",
    "@types/js-yaml": "^4.0.2",
    "@types/mustache": "^4.1.2",
    "@types/node": "^16.3.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@ulab/create-vite-macro-plugin": "^0.1.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "faker": "^5.5.3",
    "fast-glob": "^3.2.7",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "js-yaml": "^4.1.0",
    "less": "^4.1.1",
    "lint-staged": "^11.0.1",
    "msw": "^0.32.0",
    "mustache": "^4.2.0",
    "prettier": "^2.3.2",
    "shelljs": "^0.8.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.2",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-imp": "^2.0.7"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "eslintIgnore": [
    "dist/*",
    "generated/*"
  ]
}
